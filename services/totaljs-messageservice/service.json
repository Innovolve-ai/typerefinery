{
  "id": "totaljs-messageservice",
  "name": "Total JS TMS",
  "description": "TMS server",
  "enabled": true,
  "status": "30",
  "logoutput": "",
  "icon": "pi pi-globe",
  "servicetype": 10,
  "execconfig": {
    "execservice": { "id": "node" },
    "setup": {
      "win32": ["${EXEC_SERVICE_PATH}\\node_modules\\npm\\bin\\npm-cli.js i total4"],
      "darwin": ["${EXEC_SERVICE_PATH}/libs/node_modules/npm/bin/npm-cli.js i total4"],
      "linux": ["${EXEC_SERVICE_PATH}/libs/node_modules/npm/bin/npm-cli.js i total4"],
      "default": ["${EXEC_SERVICE_PATH}\\node_modules\\npm\\bin\\npm-cli.js i total4"]
    },
    "commandline": {
      "default": "index.js"
    },
    "serviceport": 8112,
    "healthcheck": {
      "type": "http",
      "url": "http://localhost:${SERVICE_PORT}/",
      "expected_status": 404,
      "retries": 30
    },
    "depend_on": ["node"]
  }
}
