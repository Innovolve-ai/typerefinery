{
  "id": "totaljs-flow",
  "name": "Total JS Flow",
  "description": "Flow server",
  "enabled": true,
  "status": "30",
  "logoutput": "",
  "icon": "pi pi-globe",
  "servicetype": 10,
  "execconfig": {
    "execservice": { "id": "node" },
    "setup": [
      "${EXEC_SERVICE_PATH}\\node-v18.6.0-win-x64\\node_modules\\npm\\bin\\npm-cli.js i total4"
    ],
    "env": {
      "SERVICE_DATA_PATH": "${SERVICE_DATA_PATH}\\database"
    },
    "commandline": "${SERVICE_PATH}\\index.js",
    "serviceport": 8111,
    "healthcheck": {
      "type": "http",
      "url": "http://127.0.0.1:${SERVICE_PORT}/",
      "expected_status": 200,
      "retries": 30
    }
  }
}
