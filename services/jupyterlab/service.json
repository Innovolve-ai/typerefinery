{
  "id": "jupyterlab",
  "name": "JupyterLab",
  "description": "JupyterLab",
  "enabled": true,
  "status": "-1",
  "icon": "pi pi-cog",
  "servicelocation": 10,
  "execconfig": {
    "execservice": { "id": "python" },
    "setup": {
      "win32": [
        "-m jupyter lab build",
        "; npm install -g ijavascript",
        "; ijsinstall --spec-path=full --working-dir=${JUPYTERLAB_JSWORK_PATH}",
        "-m jupyter kernelspec list"
      ],
      "darwin": [
        "-m jupyter lab build"
      ],
      "linux": [
        "-m jupyter lab build"
      ],
      "default": [
        "-m jupyter lab build"
      ]
    },
    "env": {
      "PYTHONPATH": "${PYTHON_HOME}",
      "NODE_HOME": "${NODE_HOME}",
      "PATH": [
          "${PYTHON_HOME}",
          "${PYTHON_SCRIPTS_PATH}",
          "${NODE_HOME}"
        ],
      "JUPYTER_DATA_DIR": "${SERVICE_PATH}",
      "JUPYTERLAB_JSWORK_PATH": "${SERVICE_PATH}/jswork",
      "JUPYTERLAB_DIR": "${SERVICE_PATH}/data",
      "JUPYTERLAB_NOTEBOOKS_DIR": "${SERVICE_PATH}/notebooks",
      "JUPYTER_RUNTIME_DIR": "${SERVICE_PATH}/runtime",
      "JUPYTER_CONFIG_DIR": "${SERVICE_PATH}/config"
    },
    "datapath": "notebooks",
    "globalenv": {
      "JUPYTERLAB_URL": "http://localhost:${SERVICE_PORT}",
      "JUPYTERLAB_PORT": "${SERVICE_PORT}"
    },
    "commandline": {
      "default": " -m jupyter lab --ServerApp.allow_remote_access=True --ServerApp.allow_origin=* --ServerApp.root_dir=${SERVICE_DATA_PATH} --no-browser --ServerApp.port=${SERVICE_PORT} --ServerApp.port_retries=0 --ServerApp.token='' --ServerApp.disable_check_xsrf=True --ServerApp.terminals_enabled=False"
    },
    "serviceport": 8888,
    "healthcheck": {
      "type": "http",
      "url": "http://localhost:${SERVICE_PORT}",
      "expected_status": 200,
      "retries": 180
    },
    "depend_on": [
      "python"
    ]
  },
  "actions": {
    "stop": {
      "commandline": {
        "default": " -m jupyter lab stop"
      }
    }
  }
}
