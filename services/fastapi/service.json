{
  "id": "fastapi",
  "name": "FastAPI",
  "description": "Fast API",
  "enabled": true,
  "status": "-1",
  "icon": "pi pi-cog",
  "servicelocation": 10,
  "execconfig": {
    "execservice": { "id": "python" },
    "setup": {
      "win32": ["-m pip install --ignore-installed --use-pep517 --no-warn-script-location --user -r ${SERVICE_PATH}\\requirements.txt"],
      "darwin": ["-m pip install --ignore-installed --use-pep517 --no-warn-script-location --user -r ${SERVICE_PATH}/requirements.txt"],
      "linux": ["-m pip install --ignore-installed --use-pep517 --no-warn-script-location --user -r ${SERVICE_PATH}/requirements.txt"],
      "default": ["-m pip install --ignore-installed --use-pep517 --no-warn-script-location --user -r ${SERVICE_PATH}\\requirements.txt"]
    },
    "env": {
      "PYTHONPATH": "${PYTHON_HOME}",
      "PYTHONUSERBASE": "${PYTHON_HOME}",
      "PATH": [
        "${PYTHON_HOME}",
        "${PYTHON_SCRIPTS_PATH}",
        "${NODE_HOME}"
      ]
    },
    "globalenv": {
      "API_URL": "http://localhost:${SERVICE_PORT}",
      "API_PORT": "${SERVICE_PORT}"
    },
    "commandline": {
      "default": "-m uvicorn --host localhost --port ${SERVICE_PORT} --app-dir ${SERVICE_PATH} main:app"
    },
    "serviceport": 8000,
    "healthcheck": {
      "type": "http",
      "url": "http://localhost:${SERVICE_PORT}/healthcheck",
      "expected_status": 200,
      "retries": 180
    },
    "depend_on": [
      "python"
    ]
  }
}
