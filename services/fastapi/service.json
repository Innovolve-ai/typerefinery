{
  "id": "fastapi",
  "name": "API",
  "description": "Fast API",
  "enabled": false,
  "status": "-1",
  "logoutput": "...",
  "icon": "pi pi-cog",
  "servicetype": 10,
  "execconfig": {
    "execservice": "python",
    "pythonrequirements": "requirements.txt",
    "commandline": "-m uvicorn main:app --host localhost --port ${SERVICE_PORT} --app-dir ${SERVICE_PATH}",
    "serviceport": 8000,
    "depend_on": [
      "python",
      "typedb"
    ],
    "healthcheck": {
      "type": "http",
      "url": "http://localhost:${SERVICE_PORT}/",
      "expected_status": 200
    }
  },
  "actions": {
    "stop": {
      "name": "stop",
      "path": "...",
      "commanline": "..."
    },
    "start": {
      "name": "start",
      "path": "...",
      "commanline": "..."
    }
  }
}
