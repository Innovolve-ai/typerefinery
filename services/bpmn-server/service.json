{
  "id": "bpmn-server",
  "name": "BPMN Server",
  "description": "BPMN Server",
  "enabled": true,
  "status": "30",
  "logoutput": "",
  "icon": "pi pi-globe",
  "servicetype": 10,
  "execconfig": {
    "execservice": { "id": "node" },
    "setup": {
      "win32": ["${EXEC_SERVICE_PATH}\\node_modules\\npm\\bin\\npm-cli.js i"],
      "darwin": ["${EXEC_SERVICE_PATH}/libs/node_modules/npm/bin/npm-cli.js i"],
      "linux": ["${EXEC_SERVICE_PATH}/libs/node_modules/npm/bin/npm-cli.js i"],
      "default": ["${EXEC_SERVICE_PATH}\\node_modules\\npm\\bin\\npm-cli.js i"]
    },
    "env": {
      "SERVICE_DATA_PATH": "${SERVICE_DATA_PATH}\\processes"
    },
    "commandline": {
      "win32": "app",
      "darwin": "app",
      "linux": "app",
      "default": "app"
    },
    "serviceport": 8190,
    "healthcheck": {
      "type": "http",
      "url": "http://127.0.0.1:${SERVICE_PORT}/",
      "expected_status": 200,
      "retries": 30
    },
    "depend_on": [
      "node",
      "websight-mongo"
    ]
  }
}
