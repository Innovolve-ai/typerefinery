{
  "id": "files",
  "name": "Files store",
  "description": "Node based file manager",
  "enabled": true,
  "status": "30",
  "logoutput": "",
  "icon": "pi pi-globe",
  "servicetype": 10,
  "execconfig": {
    "execservice": { "id": "node" },
    "setup": {
      "win32": ["; ${NPM} install"],
      "darwin": ["; ${NPM} install"],
      "linux": ["; ${NPM} install"],
      "default": ["; ${NPM} install"]
    },
    "datapath": "./data",
    "globalenv": {
      "FLOW_HOST_URL": "http://localhost:${SERVICE_PORT}",
      "FLOW_PORT": "${SERVICE_PORT}"
    },
    "commandline": {
      "win32": "${SERVICE_PATH}\\lib\\index.js",
      "darwin": "${SERVICE_PATH}/lib/index.js",
      "linux": "${SERVICE_PATH}/lib/index.js",
      "default": "${SERVICE_PATH}\\lib\\index.js"
    },
    "serviceport": 8199,
    "healthcheck": {
      "type": "http",
      "url": "http://127.0.0.1:${SERVICE_PORT}/",
      "expected_status": 200,
      "retries": 180
    },
    "depend_on": [
      "node"
    ]
  }
}
