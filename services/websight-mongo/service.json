{
  "id": "websight-mongo",
  "name": "Websight Mongo",
  "description": "Mongo instance for Websight",
  "enabled": false,
  "status": "-1",
  "logoutput": "",
  "icon": "pi pi-cog",
  "servicetype": 10,
  "serviceorder": 0,
  "execconfig": {
    "serviceorder": 1,
    "serviceport": 8180,
    "setuparchive": {
      "win32": {
        "name": "win32.zip",
        "output": "win32"
      },
      "darwin": {
        "name": "drwin.zip",
        "output": "drwin"
      },
      "linux": {
        "name": "linux.zip",
        "output": "linux"
      }
    },
    "executable": {
      "win32": "bin\\mongod.exe",
      "darwin": "bin/mongod",
      "linux": "bin/mongod",
      "default": "bin\\mongod.exe"
    },
    "commandline": {
      "win32": "--dbpath ${SERVICE_HOME}\\database --bind_ip_all --auth",
      "darwin": "--dbpath ${SERVICE_HOME}/database --bind_ip_all --auth",
      "linux": "--dbpath ${SERVICE_HOME}/database --bind_ip_all --auth",
      "default": "--dbpath ${SERVICE_HOME}\\database --bind_ip_all --auth"
    }
  }
}
