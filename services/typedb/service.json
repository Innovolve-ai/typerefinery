{
  "id": "typedb",
  "name": "TypeDB",
  "description": "Database",
  "enabled": true,
  "status": "30",
  "icon": "pi pi-database",
  "servicelocation": 10,
  "execconfig": {
    "execservice": { "id": "java" },
    "execshell": false,
    "execcwd": "${SERVICE_HOME}",
    "setuparchive": {
      "win32": {
        "name": "typedb-all-windows-x86_64-2.25.6.7z.001",
        "output": "typedb"
      },
      "darwin": {
        "name": "typedb-all-darwin-2.22.0.7z.001",
        "output": "typedb"
      },
      "linux": {
        "name": "typedb-all-linux-2.22.0.7z.001",
        "output": "typedb"
      }
    },
    "commandline": {
      "win32": "-Xms256m -Xmx1024m -cp \"${SERVICE_BIN_PATH}\\server\\lib\\*;\" -Dtypedb.dir=\"${SERVICE_BIN_PATH}\" com.vaticle.typedb.core.server.TypeDBServer --config=\"${SERVICE_BIN_PATH}\\server\\conf\\config.yml\" --storage.data=\"${SERVICE_DATA_PATH}\" --server.address 0.0.0.0:${SERVICE_PORT}",
      "darwin": "-Xms256m -Xmx1024m -cp \"${SERVICE_BIN_PATH}/server/lib/*\" -Dtypedb.dir=\"${SERVICE_BIN_PATH}\" com.vaticle.typedb.core.server.TypeDBServer --config=\"${SERVICE_BIN_PATH}/server/conf/config.yml\" --storage.data=\"${SERVICE_DATA_PATH}\" --server.address 0.0.0.0:${SERVICE_PORT}",
      "linux": "-Xms256m -Xmx1024m -cp \"${SERVICE_BIN_PATH}/server/lib/*\" -Dtypedb.dir=\"${SERVICE_BIN_PATH}\" com.vaticle.typedb.core.server.TypeDBServer --config=\"${SERVICE_BIN_PATH}/server/conf/config.yml\" --storage.data=\"${SERVICE_DATA_PATH}\" --server.address 0.0.0.0:${SERVICE_PORT}",
      "default": "-Xms256m -Xmx1024m -cp \"${SERVICE_BIN_PATH}\\server\\lib\\*\" -Dtypedb.dir=\"${SERVICE_BIN_PATH}\" com.vaticle.typedb.core.server.TypeDBServer --config=\"${SERVICE_BIN_PATH}\\server\\conf\\config.yml\" --storage.data=\"${SERVICE_DATA_PATH}\" --server.address 0.0.0.0:${SERVICE_PORT}"
    },
    "commandlinecli": {
      "win32": "-Xms256m -Xmx1024m -cp \"${SERVICE_BIN_PATH}\\console\\conf\\;${SERVICE_BIN_PATH}\\console\\lib\\*;\" -D\"org.jline.terminal.dumb=true\" -Dtypedb.dir=\"${SERVICE_BIN_PATH}\" com.vaticle.typedb.console.TypeDBConsole --server 0.0.0.0:${SERVICE_PORT} ",
      "darwin": "-Xms256m -Xmx1024m -cp \"${SERVICE_BIN_PATH}/console/conf/:${SERVICE_BIN_PATH}/console/lib/*:\" -D\"org.jline.terminal.dumb=true\" -Dtypedb.dir=\"${SERVICE_BIN_PATH}\" com.vaticle.typedb.console.TypeDBConsole --server 0.0.0.0:${SERVICE_PORT} ",
      "linux": "-Xms256m -Xmx1024m -cp \"${SERVICE_BIN_PATH}/console/conf/:${SERVICE_BIN_PATH}/console/lib/*:\" -D\"org.jline.terminal.dumb=true\" -Dtypedb.dir=\"${SERVICE_BIN_PATH}\" com.vaticle.typedb.console.TypeDBConsole --server 0.0.0.0:${SERVICE_PORT} ",
      "default": "-Xms256m -Xmx1024m -cp \"${SERVICE_BIN_PATH}\\console\\conf\\;${SERVICE_BIN_PATH}\\console\\lib\\*;\" -D\"org.jline.terminal.dumb=true\" -Dtypedb.dir=\"${SERVICE_BIN_PATH}\" com.vaticle.typedb.console.TypeDBConsole --server 0.0.0.0:${SERVICE_PORT} "
    },
    "datapath": "/server/data",
    "serviceport": 8729,
    "healthcheck": {
      "type": "tcp",
      "retries": 30
    },
    "env": {
      "PATH": [
        "${ARCHIVE_HOME}",
        "${JAVA_HOME}"
      ]
    },
    "globalenv": {
      "TYPEDB_DB": "typerefinery",
      "TYPEDB_PORT": "${SERVICE_PORT}",
      "TYPEDB_HOST": "${SERVICE_HOST}"
    },
    "depend_on": [
      "java"
    ]
  }
}
