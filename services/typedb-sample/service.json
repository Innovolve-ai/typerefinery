{
  "id": "typedb-sample",
  "name": "TypeDB Sample",
  "description": "TypeDB Sample Data",
  "enabled": true,
  "status": "-1",
  "icon": "pi pi-cog",
  "servicetype": 50,
  "servicelocation": 10,
  "execconfig": {
    "execservice": { "id": "python" },
    "setup": {
      "win32": [
        "-m pip install --user -r ${SERVICE_PATH}\\requirements.txt",
        "${SERVICE_PATH}\\basic_upload.py --port ${TYPEDB_PORT} --db ${TYPEDB_DB} --host ${TYPEDB_HOST}"
      ],
      "darwin": [
        "-m pip install --user_ -r ${SERVICE_PATH}/requirements.txt",
        "${SERVICE_PATH}/basic_upload.py --port ${TYPEDB_PORT} --db ${TYPEDB_DB} --host ${TYPEDB_HOST}"
      ],
      "linux": [
        "-m pip install --user -r ${SERVICE_PATH}/requirements.txt",
        "${SERVICE_PATH}/basic_upload.py --port ${TYPEDB_PORT} --db ${TYPEDB_DB} --host ${TYPEDB_HOST}"
      ],
      "default": [
        "-m pip install --user -r ${SERVICE_PATH}\\requirements.txt",
        "${SERVICE_PATH}\\basic_upload.py --port ${TYPEDB_PORT} --db ${TYPEDB_DB} --host ${TYPEDB_HOST}"
      ]
    },
    "commandline": {
      "default": ""
    },
    "env": {
      "LOGURU_LEVEL": "INFO",
      "PYTHONPATH": "${PYTHON_HOME}",
      "PYTHONUSERBASE": "${PYTHON_HOME}",
      "PATH": [
        "${PYTHON_HOME}",
        "${PYTHON_SCRIPTS_PATH}"
      ]
    },
    "depend_on": [
      "typedb",
      "typedb-init"
    ]
  }
}
