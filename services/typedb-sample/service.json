{
  "id": "typedb-sample",
  "name": "TypeDB Sample",
  "description": "TypeDB Sample Data",
  "enabled": true,
  "status": "-1",
  "logoutput": "...",
  "icon": "pi pi-cog",
  "servicetype": 10,
  "execconfig": {
    "execservice": { "id": "python" },
    "setup": {
      "win32": [
        "-m pip install --target=${SERVICE_PATH}\\__packages__ -r ${SERVICE_PATH}\\requirements.txt",
        "${SERVICE_PATH}\\basic_upload.py --port 8179"
      ],
      "darwin": [
        "-m pip install --target=${SERVICE_PATH}/__packages__ -r ${SERVICE_PATH}/requirements.txt",
        "${SERVICE_PATH}/basic_upload.py --port 8179"
      ],
      "linux": [
        "-m pip install --target=${SERVICE_PATH}/__packages__ -r ${SERVICE_PATH}/requirements.txt",
        "${SERVICE_PATH}/basic_upload.py --port 8179"
      ],
      "default": [
        "-m pip install --target=${SERVICE_PATH}\\__packages__ -r ${SERVICE_PATH}\\requirements.txt",
        "${SERVICE_PATH}\\basic_upload.py --port 8179"
      ]
    },
    "commandline": {
      "default": ""
    },
    "serviceport": 8000,
    "depend_on": [
      "typedb"
    ]
  }
}
