{
  "id": "postgredb-admin",
  "name": "pdAdmin4",
  "description": "Web Admin UI for PostgreDB",
  "enabled": true,
  "status": "-1",
  "icon": "pi pi-cog",
  "servicetype": 10,
  "execconfig": {
    "execservice": { "id": "python" },
    "setup": {
      "win32": [
        "-m pip install --ignore-installed --use-pep517 --user -r ${SERVICE_PATH}\\requirements.txt",
        "; copy ${SERVICE_PATH}\\config\\config_local.py ${PYTHON_USERBASE_PATH_SCRIPTS_PATH}\\pgadmin4\\config_local.py"
      ],
      "darwin": [
        "-m pip install --ignore-installed --use-pep517 --user -r ${SERVICE_PATH}/requirements.txt",
        "; /bin/cp -r ${SERVICE_PATH}/config/config_local.py ${PYTHON_USERBASE_PATH_SCRIPTS_PATH}/pgadmin4/config_local.py"
      ],
      "linux": [
        "-m pip install --ignore-installed --use-pep517 --user -r ${SERVICE_PATH}/requirements.txt",
        "; /bin/cp -r ${SERVICE_PATH}/config/config_local.py ${PYTHON_USERBASE_PATH_SCRIPTS_PATH}/pgadmin4/config_local.py"
      ],
      "default": [
        "-m pip install --ignore-installed --use-pep517 --user -r ${SERVICE_PATH}\\requirements.txt",
        "; copy ${SERVICE_PATH}\\config\\config_local.py ${PYTHON_USERBASE_PATH_SCRIPTS_PATH}\\pgadmin4\\config_local.py"
      ]
    },
    "env": {
      "DATA_DIR": "${SERVICE_DATA_PATH}",
      "PGADMIN_SCRIPT": "${PYTHON_USERBASE_PATH_SCRIPTS_PATH}/pgadmin4/pgAdmin4.py",
      "PGADMIN_SCRIPT_HOME": "${PYTHON_USERBASE_PATH_SCRIPTS_PATH}/pgadmin4",
      "PGADMIN_SERVER_MODE": "OFF",
      "PGADMIN_INT_PORT": "${SERVICE_PORT}",
      "CONSOLE_LOG_LEVEL": "INFO",
      "FILE_LOG_LEVEL": "INFO",
      "PYTHONPATH": "${PYTHON_HOME}",
      "PYTHONUSERBASE": "${PYTHON_USERBASE_PATH}",
      "PATH": [
        "${PYTHON_HOME}",
        "${PYTHON_SCRIPTS_PATH}",
        "${PYTHON_USERBASE_PATH_SCRIPTS}",
        "${NODE_HOME}"
      ]
    },
    "datapath": "data",
    "globalenv": {
      "PGADMIN_URL": "http://localhost:${SERVICE_PORT}",
      "PGADMIN_PORT": "${SERVICE_PORT}"
    },
    "commandline": {
      "win32": "${SERVICE_PATH}\\main.py",
      "darwin": "${SERVICE_PATH}/main.py",
      "linux": "${SERVICE_PATH}/main.py",
      "default": "${SERVICE_PATH}\\main.py"
    },
    "serviceport": 8510,
    "healthcheck": {
      "type": "http",
      "url": "http://localhost:${SERVICE_PORT}/healthcheck",
      "expected_status": 200,
      "retries": 180
    },
    "depend_on": [
      "python"
    ]
  }
}
