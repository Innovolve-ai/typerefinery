{
  "id": "postgredb-admin",
  "name": "pdAdmin4",
  "description": "Web Admin UI for PostgreDB",
  "enabled": true,
  "status": "-1",
  "icon": "pi pi-cog",
  "servicetype": 10,
  "execconfig": {
    "execservice": { "id": "python" },
    "setup": {
      "win32": [
        "-m pip install --ignore-installed --use-pep517 \"parse (==1.19.0)\" --target=${SERVICE_PATH}\\__packages__ -r ${SERVICE_PATH}\\requirements.txt",
        "; copy -r ${SERVICE_PATH}\\config\\config_local.py ${SERVICE_PATH}\\__packages__\\pgadmin4\\config_local.py"
      ],
      "darwin": [
        "-m pip install --ignore-installed --use-pep517 \"parse (==1.19.0)\" --target=${SERVICE_PATH}/__packages__ -r ${SERVICE_PATH}/requirements.txt",
        "; /bin/cp -r ${SERVICE_PATH}/config/config_local.py ${SERVICE_PATH}/__packages__/pgadmin4/config_local.py"
      ],
      "linux": [
        "-m pip install --ignore-installed --use-pep517 \"parse (==1.19.0)\" --target=${SERVICE_PATH}/__packages__ -r ${SERVICE_PATH}/requirements.txt",
        "; cp -r ${SERVICE_PATH}/config/config_local.py ${SERVICE_PATH}/__packages__/pgadmin4/config_local.py"
      ],
      "default": [
        "-m pip install --ignore-installed --use-pep517 \"parse (==1.19.0)\" --target=${SERVICE_PATH}\\__packages__ -r ${SERVICE_PATH}\\requirements.txt",
        "; copy -r ${SERVICE_PATH}\\config\\config_local.py ${SERVICE_PATH}\\__packages__\\pgadmin4\\config_local.py"
      ]
    },
    "env": {
      "DATA_DIR": "${SERVICE_DATA_PATH}",
      "PYTHONPATH": "${PYTHON_HOME}",
      "PATH": "${SERVICE_PATH}/__packages__"
    },
    "datapath": "data",
    "globalenv": {
      "API_URL": "http://localhost:${SERVICE_PORT}",
      "API_PORT": "${SERVICE_PORT}"
    },
    "commandline": {
      "win32": "${SERVICE_PATH}\\__packages__\\pgadmin4\\pgadmin4.py",
      "darwin": "${SERVICE_PATH}/__packages__/pgadmin4/pgadmin4.py",
      "linux": "${SERVICE_PATH}/__packages__/pgadmin4/pgadmin4.py",
      "default": "${SERVICE_PATH}\\__packages__\\pgadmin4\\pgadmin4.py"
    },
    "serviceport": 8510,
    "healthcheck": {
      "type": "http",
      "url": "http://localhost:${SERVICE_PORT}/healthcheck",
      "expected_status": 200,
      "retries": 180
    },
    "depend_on": [
      "python"
    ]
  }
}
