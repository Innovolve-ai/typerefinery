{
  "id": "python",
  "name": "Python 3.10",
  "description": "Embeded Python 3.10",
  "enabled": true,
  "status": "-1",
  "logoutput": "",
  "icon": "pi pi-cog",
  "servicetype": 50,
  "servicelocation": 10,
  "execconfig": {
    "serviceorder": 1,
    "executable": {
      "win32": "win32\\python.exe",
      "darwin": "darwin/bin/python",
      "linux": "linux/bin/python",
      "default": "win32\\python.exe"
    },
    "setup": {
      "win32": [
        "get-pip.py --no-warn-script-location",
        "-m pip install --ignore-installed --no-warn-script-location --use-pep517 --user -r ${SERVICE_HOME}\\win32\\requirements.txt"
      ],
      "darwin": [
        "-m pip install --ignore-installed --no-warn-script-location --use-pep517 --user -r ${SERVICE_HOME}/darwin/requirements.txt"
      ],
      "linux": [
        "-m pip install --ignore-installed --no-warn-script-location --use-pep517 --user -r ${SERVICE_HOME}/linux/requirements.txt"
      ],
      "default": [
        "get-pip.py --no-warn-script-location",
        "-m pip install --ignore-installed --no-warn-script-location --use-pep517 --user -r ${SERVICE_HOME}\\win32\\requirements.txt"
      ]
    },
    "commandline": {
      "default": " -c \"import os,sysconfig;print('PYTHON_SCRIPTS_PATH:'+sysconfig.get_path('scripts',f'{os.name}_user'))\""
    },
    "outputvarregex": {
      "PYTHON_SCRIPTS_PATH": "PYTHON_SCRIPTS_PATH:(.*)"
    },
    "env": {
      "PATH": "${SERVICE_EXECUTABLE_HOME}",
      "PYTHON_HOME": "${SERVICE_EXECUTABLE_HOME}"
    },
    "globalenv": {
      "PYTHON": "${SERVICE_EXECUTABLE}",
      "PYTHON_HOME": "${SERVICE_EXECUTABLE_HOME}"
    }
  }
}
