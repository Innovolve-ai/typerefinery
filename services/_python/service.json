{
  "id": "python",
  "name": "Python 3.10",
  "description": "Embeded Python 3.10",
  "enabled": true,
  "status": "-1",
  "logoutput": "",
  "icon": "pi pi-cog",
  "servicetype": 50,
  "servicelocation": 10,
  "execconfig": {
    "serviceorder": 1,
    "executable": {
      "win32": "win32\\python.exe",
      "darwin": "darwin/bin/python",
      "linux": "linux/bin/python",
      "default": "win32\\python.exe"
    },
    "setup": {
      "win32": [
        "get-pip.py --no-warn-script-location",
        "-m pip install --ignore-installed --no-warn-script-location -r ${SERVICE_PATH}\\requirements.txt"
      ],
      "darwin": [
        "-m pip install --ignore-installed --no-warn-script-location -r ${SERVICE_PATH}/requirements.txt"
      ],
      "linux": [
        "-m pip install --ignore-installed --no-warn-script-location -r ${SERVICE_PATH}/requirements.txt"
      ],
      "default": [
        "get-pip.py --no-warn-script-location",
        "-m pip install --ignore-installed --no-warn-script-location -r ${SERVICE_PATH}\\requirements.txt"
      ]
    },
    "globalenv": {
      "PYTHON": "${SERVICE_EXECUTABLE}"
    }
  }
}
